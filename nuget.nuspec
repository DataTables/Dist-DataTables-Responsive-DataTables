<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>datatables.net-responsive-dt</id>
    <version>2.2.5</version>
    <description>Responsive for DataTables</description>
    <authors>SpryMedia Ltd</authors>
    <projectUrl>http://datatables.net</projectUrl>
    <license type="expression">MIT</license>
    <tags>responsive DataTables jQuery table DataTables</tags>
    <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Responsive-DataTables.git"/>
    <dependencies>
      <dependency id="jquery" version="1.7.0"/>
      <dependency id="datatables.net" version="1.10.15"/>
      <dependency id="datatables.net-responsive" version="2.2.5"/>
      <dependency id="datatables.net-dt" version="1.10.15"/>
    </dependencies>
    <contentFiles>
      <files include="**/*" buildAction="Content"/>
    </contentFiles>
  </metadata>
  <files>
    <file src="js/responsive.datatables.js" target="content\Scripts"/>
    <file src="js/responsive.datatables.min.js" target="content\Scripts"/>
    <file src="css/responsive.datatables.css" target="content\Content"/>
    <file src="css/responsive.datatables.min.css" target="content\Content"/>
    <file src="js/responsive.datatables.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="js/responsive.datatables.min.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="css/responsive.datatables.css" target="contentFiles\any\any\wwwroot\css"/>
    <file src="css/responsive.datatables.min.css" target="contentFiles\any\any\wwwroot\css"/>
  </files>
</package>
